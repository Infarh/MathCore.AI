<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.1.0</Version>
    <Authors>Shmachilin P.A.</Authors>
    <Company />
    <Description>Библиотека алгоритмов машинного обучения и искусственного интеллекта</Description>
    <PackageLicenseFile>license.txt</PackageLicenseFile>
    <AssemblyVersion>0.0.1.0</AssemblyVersion>
    <FileVersion>0.0.1.0</FileVersion>
    <PackageReleaseNotes>Альфа-версия пакета</PackageReleaseNotes>
    <LangVersion>8</LangVersion>
    <PackageProjectUrl>https://github.com/Infarh/MathCore.AI</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Infarh/MathCore.AI</RepositoryUrl>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>MathCore.AI.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MathCore" Version="0.0.4" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\license.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
