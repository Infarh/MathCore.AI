<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.1.3</Version>
    <Authors>Shmachilin P.A.</Authors>
    <Company />
    <Description>Библиотека алгоритмов машинного обучения и искусственного интеллекта</Description>
    <PackageLicenseFile>license.txt</PackageLicenseFile>
    <AssemblyVersion>0.0.1.3</AssemblyVersion>
    <FileVersion>0.0.1.3</FileVersion>
    <PackageReleaseNotes>Подписывание сборки</PackageReleaseNotes>
    <LangVersion>8</LangVersion>
    <PackageProjectUrl>https://github.com/Infarh/MathCore.AI</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Infarh/MathCore.AI</RepositoryUrl>
    <Copyright>shmachilin@gmail.com</Copyright>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>MathCoreAISign.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile></DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MathCore" Version="0.0.8" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\license.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
